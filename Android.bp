cc_binary {

    name: "AndroidPerfServer",
    srcs: [
        "server.cpp",
        "main.cpp",
    ],
    shared_libs: [
        "libbase",
        "libutils",
        "liblog",
        "libbinder",
        "libcutils",
    ],
    cflags: [
        "-Wall",
        "-Werror",
    ],

    compile_multilib: "both",
    multilib: {
        lib32: {
            stem: "AndroidPerfServer32",
        },
        lib64: {
            stem: "AndroidPerfServer64",
        },
    },

}